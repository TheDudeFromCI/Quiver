{
  "root": "packages/quiver-graph-logic",
  "sourceRoot": "packages/quiver-graph-logic/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/quiver-graph-logic",
        "main": "packages/quiver-graph-logic/src/index.ts",
        "tsConfig": "packages/quiver-graph-logic/tsconfig.lib.json",
        "assets": [
          "packages/quiver-graph-logic/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/quiver-graph-logic/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/quiver-graph-logic"
      ],
      "options": {
        "jestConfig": "packages/quiver-graph-logic/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
